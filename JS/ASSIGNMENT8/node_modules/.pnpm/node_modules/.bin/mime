#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/THE_JITU/JS/ASSIGNMENT8/node_modules/.pnpm/mime@4.0.4/node_modules/mime/bin/node_modules:/mnt/d/THE_JITU/JS/ASSIGNMENT8/node_modules/.pnpm/mime@4.0.4/node_modules/mime/node_modules:/mnt/d/THE_JITU/JS/ASSIGNMENT8/node_modules/.pnpm/mime@4.0.4/node_modules:/mnt/d/THE_JITU/JS/ASSIGNMENT8/node_modules/.pnpm/node_modules:/mnt/d/THE_JITU/JS/ASSIGNMENT8/node_modules:/mnt/d/THE_JITU/JS/node_modules:/mnt/d/THE_JITU/node_modules:/mnt/d/node_modules:/mnt/d/THE_JITU/js/assignment8/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/THE_JITU/JS/ASSIGNMENT8/node_modules/.pnpm/mime@4.0.4/node_modules/mime/bin/node_modules:/mnt/d/THE_JITU/JS/ASSIGNMENT8/node_modules/.pnpm/mime@4.0.4/node_modules/mime/node_modules:/mnt/d/THE_JITU/JS/ASSIGNMENT8/node_modules/.pnpm/mime@4.0.4/node_modules:/mnt/d/THE_JITU/JS/ASSIGNMENT8/node_modules/.pnpm/node_modules:/mnt/d/THE_JITU/JS/ASSIGNMENT8/node_modules:/mnt/d/THE_JITU/JS/node_modules:/mnt/d/THE_JITU/node_modules:/mnt/d/node_modules:/mnt/d/THE_JITU/js/assignment8/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/bin/cli.js" "$@"
else
  exec node  "$basedir/../mime/bin/cli.js" "$@"
fi
